{
  "Logging": {
    "File": {
      "IncludeScopes": true,
      "BasePath": "Logs",
      "MaxFileSize": 256,
      "Files": [
        {
          "Path": "log-<counter>.log",
          "MaxFiles": 3
        },
        {
          "Path": "log-all.log"
        }
      ]
    },
    "LogLevel": {
      "Default": "Trace"
    }
  }
}
